{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk:deploy": "npm run build && cdk deploy --all",
    "cdk:diff": "env && npm run build && cdk diff --all",
    "cdk:destroy": "npm run build && cdk destroy --all",

    "build:frontend": "cd ../frontend && ./ci-build.sh && cd -",

    "cdk:deploy-frontend": "npm run build && cdk deploy eu-west-1-zoorl-webfrontend-stack",
    "cdk:diff-frontend": "npm run build && cdk diff eu-west-1-zoorl-webfrontend-stack",
    "cdk:destroy-frontend": "npm run build && cdk destroy eu-west-1-zoorl-webfrontend-stack"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.117.0",
    "@types/jest": "26.0.24",
    "@types/node": "10.17.27",
    "aws-cdk": "1.117.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.117.0",
    "@aws-cdk/aws-certificatemanager": "^1.117.0",
    "@aws-cdk/aws-cloudfront": "^1.117.0",
    "@aws-cdk/aws-cognito": "1.117.0",
    "@aws-cdk/aws-dynamodb": "1.117.0",
    "@aws-cdk/aws-iam": "^1.117.0",
    "@aws-cdk/aws-lambda": "1.117.0",
    "@aws-cdk/aws-lambda-python": "1.117.0",
    "@aws-cdk/aws-route53": "^1.117.0",
    "@aws-cdk/aws-route53-patterns": "^1.117.0",
    "@aws-cdk/aws-route53-targets": "^1.117.0",
    "@aws-cdk/aws-s3": "^1.117.0",
    "@aws-cdk/aws-s3-deployment": "^1.117.0",
    "@aws-cdk/core": "1.117.0",
    "@aws-cdk/pipelines": "1.117.0",
    "cdk-spa-deploy": "^1.104.1",
    "source-map-support": "^0.5.16"
  }
}
